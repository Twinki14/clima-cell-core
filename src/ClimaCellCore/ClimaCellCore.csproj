<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <RepositoryUrl>https://github.com/Twinki14/clima-cell-core</RepositoryUrl>
    <PackageTags>csharp,climacell,clima-cell,weather,weather-api,wrapper,library</PackageTags>
    <PackageProjectUrl>https://github.com/Twinki14/clima-cell-core</PackageProjectUrl>
    <OutputType>Library</OutputType>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Authors>Twinki</Authors>
    <Product>clima-cell-core</Product>
